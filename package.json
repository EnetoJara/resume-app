    "scripts": {
        "clean": "rimraf dist && rimraf reports && rimraf build",
        "muta": "stryker run",
        "debug": "npm run start && nodemon --inspect ./dist/index.js 80",
        "pretty": "prettier ./config/*.ts ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts --color --write",
        "prod:pretty": "prettier ./dist/*.js ./dist/**/*.js ./dist/**/**/*.js ./dist/**/**/**/*.js ./dist/**/**/**/**/*.js --color --write",
        "prefix": "rimraf dist && rimraf reports && rimraf dist && npm run pretty",
        "fix": "tslint -c tslint.json -p ./tsconfig.json ./config/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts ./src/*.ts --fix",
        "predev": "tslint -c tslint.json -p ./tsconfig.json ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts ./src/*.ts --fix",
        "babel": "NODE_ENV=production babel --config-file ./babel.config.js ./src --extensions .ts --out-dir ./dist",
        "docker": "docker run --rm --name pg-docker -e POSTGRES_PASSWORD=docker -d -p 54320:5432 -v $HOME/docker/volumes/postgres:/var/lib/postgresql/data  postgres",
        "login": "psql -h localhost -p 54320 -U -dadoptApi",
        "build:dev": "NODE_ENV=production BUILDING=true webpack --config=config/webpack.dev-server.js --env.NODE_ENV=production",
        "dev:debug": "GENERATE_SOURCEMAP=true NODE_ENV=development BUILDING=false nodemon --inspect --watch server --watch config server/index.js --env.NODE_ENV=development",
        "dev": "NODE_ENV=development GENERATE_SOURCEMAP=true BUILDING=false nodemon --watch server --watch config server/index.js",
        "server": "nodemon",
        "prod": "NODE_ENV=production node ./production/server/index.js",
        "start": "NODE_ENV=production webpack-dev-server --config=config/webpack.server.js --env.NODE_ENV=production",
        "db:create": "rm -rf ./src/db/**/*.ts && tbabel --config-file ./babel.config.js ./src/db/** --extensions .ts && sequelize-cli db:create ",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev:server": "nodemon --config ./nodemon.server.json",
        "dev:client": "NODE_ENV=development webpack --mode=development --config=config/webpack.client.js --watch",
        "build:server": "NODE_ENV=production webpack --config=config/webpack.server.js --mode=production --color",
        "postprod:server": "npm run prod:pretty",
        "build:client": "NODE_ENV=production webpack --mode=production --config=config/webpack.client"
    },
