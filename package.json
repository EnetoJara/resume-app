    "scripts": {
        "clean": "rimraf dist && rimraf reports && rimraf build && rimraf tsconfig.*.tsbuildinfo",
        "muta": "NODE_ENV=test stryker run",
        "tsc:server": "tsc -p ./tsconfig.server.json",
        "pretty:styles": "prettier-stylelint ./config/*.js ./src/client/hoc/**/* ./src/client/components/**/* --color --write",
        "pretty:code": "prettier ./src/*.{ts,tsx} ./src/**/*.{ts,tsx} ./src/**/**/*.{ts,tsx} ./src/**/**/**/*.{ts,tsx} ./src/**/**/**/**/*.{ts,tsx} ./src/**/**/**/**/**/*.{ts,tsx} --color --write",
        "pretty:test": "prettier ./mocha/**/*.test.ts --color --write",
        "pretty:util": "prettier ./dist/*.js ./config/* ./babel.config.js --color --write",
        "pretty": "run-p -l pretty:*",
        "babel:server": "npm run clean && NODE_ENV=production IS_SERVER=true babel --config-file ./babel.config.js ./src --extensions .ts --out-dir ./dist && npm run type-check:server",
        "server:webpack": "NODE_ENV=development IS_SERVER=true webpack --mode=production --config=config/webpack.server.js --color --watch",
        "server:demon": "nodemon",
        "server": "run-p -l server:*",
        "fix:client": "eslint --config ./.eslint.client.json ./src/client/*.{tsx,ts} ./src/client/**/*.{tsx,ts} ./src/client/**/**/*.{tsx,ts} ./src/client/**/**/**/*.{tsx,ts} --fix",
        "fix:server": "eslint --config ./.eslint.server.json ./src/server/*.ts ./src/server/**/*.ts ./src/server/**/**/*.ts ./src/server/**/**/**/*.ts --fix",
        "fix:styles": "stylelint --config ./.stylelintrc.js ./src/client/styles/*.scss -s scss --fix",
        "fix": "npm run clean && run-p -l fix:*",
        "client": "NODE_ENV=development IS_SERVER=false webpack-dev-server --config=config/webpack.prod.js --mode=development --color",
        "type-check:client": "tsc -p ./tsconfig.client.json",
        "type-check:server": "tsc -p ./tsconfig.server.json",
        "type-check": "run-p -l type-check:*"
    },
